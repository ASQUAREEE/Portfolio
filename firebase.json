{
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "dist", // Update this to the directory where your built Vue.js app is located
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
